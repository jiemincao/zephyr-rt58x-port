config SOC_FAMILY_RT58X
        bool

config SOC_SERIES_RT582
        bool
        select SOC_FAMILY_RT58X

config SOC_FAMILY
        default "rt58x" if SOC_FAMILY_RT58X

config SOC_SERIES
        default "rt582" if SOC_SERIES_RT582

config SOC
        default "rt582" if SOC_RT582

rsource "*/Kconfig.soc"
